{
  "name": "nawa-rate",
  "version": "0.1.0",
  "description": "Framework-agnostic rate limiting middleware for Next.js, Express, and beyond",
  "keywords": [
    "rate-limit",
    "rate-limiting",
    "middleware",
    "nextjs",
    "express",
    "redis",
    "typescript"
  ],
  "homepage": "https://github.com/nawadotdev/nawa-rate#readme",
  "bugs": {
    "url": "https://github.com/nawadotdev/nawa-rate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nawadotdev/nawa-rate.git"
  },
  "license": "MIT",
  "author": "nawadotdev",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js",
      "require": "./dist/express.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "peerDependencies": {
    "ioredis": ">=5.0.0",
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ioredis": "^5.3.2",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
